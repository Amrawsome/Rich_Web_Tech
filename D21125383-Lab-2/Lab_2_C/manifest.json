{
    "manifest_version": 3,
    "name": "catsAreTheBest",
    "version": "1.0.0",
    "permissions":[
        "activeTab",
        "<all_urls>",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
      },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]

        }
    ] 
    
}